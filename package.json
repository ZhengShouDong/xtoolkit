    "inquirer": "^2.0.0",
    "semver": "^5.3.0",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.9.0"
  }
